/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

// a || sel =>

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notSel);

    And(a=a, b=sel, out=AandSel);
    And(a=notSel, b=b, out=BandnotSel);
    Or(a=AandSel, b=BandnotSel, out=out);
}
